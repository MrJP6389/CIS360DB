create table Account (
	ADDRESS 	VARCHAR(20) 	NOT NULL
	CardNumber 	NUMBER 		NOT NULL
	PLAYLIST 	VARCHAR(10) 
	FAVORITEARTIST 	VARCHAR(15) 
	EMAIL 		VARCHAR(20) 	NOT NULL
	ACCOUNTID 	VARCHAR(20) 	NOT NULL
	NAME 		VARCHAR(10) 	NOT NULL
	BIRTHDATE 	DATE 		NOT NULL
	PRIMARY KEY (ACCOUNTID));

CREATE TABLE SUBSCRIPTION(
	SUBID 		 VARCHAR(5) 	NOT NULL
	FAVORITEARTIST 	 VARCHAR(10)
	EDITSUBSCRIPTION NUMBER 
	SUBTYPE 	 NUMBER 	NOT NULL
	CARDNUMBER 	 NUMBER 	NOT NULL
	BILLADDRESS 	 VARCHAR(20) 	NOT NULL
	PRIMARY KEY (SUBID)
	FORIEGN KEY (CARDNUMBER) REFERENCES ACCOUNT(CardNumber)
	FORIEGN KEY (BILLADRRESS) REFERENCES ACCOUNT(ADRRESS));

CREATE TABLE ARTIST(
	ARTISTID 	VARCHAR(5) 	NOT NULL
	FIRSTNAME 	VARCHAR(10) 	NOT NULL
	LASTNAME 	VARCHAR(10)
	PRIMARY KEY (ARTISTID));

CREATE TABLE SONG(
	SONGID 		VARCHAR(5) 	NOT NULL
	TITLE 		VARCHAR(20) 	NOT NULL
	ARTIST 		VARCHAR(5) 	NOT NULL
	ALBUM 		VARCHAR(5) 
	RELEASEDATE 	DATE 		NOT NULL
	SINGLE 		BOOLEAN
	LENGTH 		DECIMAL 	NOT NULL
	PRIMARY KEY (SONGID)
	FORIEGN KEY (ARTIST) REFERENCES ARTIST(ARTISTID)
	FORIEGN KEY (ALBUM) REFERENCES ALBUM(ALBUMID));
CREATE TABLE ALBUM(
	ALBUMID 	VARCHAR(5) 	NOT NULL
	NOOFSONGS 	NUMBER 		NOT NULL
	SONGS 		VARCHAR(5) 	NOT NULL
	TITLE 		VARCHAR(20) 	NOT NULL
	ARTIST 		VARHAR(5) 	NOT NULL
	RELEASEDATE 	DATE 		NOT NULL
	PRIMARY KEY (ALBUMID)
	FORIEGN KEY (SONGS) REFERENCES SONG(SONGID)
	FORIEGN KEY (ARTIST) REFERENCES ARTIST(ARTISTID));
	
